window.onload=function(){
  CheckInterval();
}
var CheckInterval=setInterval(function(){
  if(window.jQuery){
    clearInterval(CheckInterval);
    LoadSRClickHandlers();
    GetBaseModelData();
    GetProductsForSearch();
    if(screen.width > 800)
    {
      $("#dash_card").css('display','flex');
      $(".card").css('margin-right', '20px');
    }
    else
    {           
      $("#dash_card").css('display','block');          
    }

    $('#CreateInvoice').hide();
    $('#CustomDiscount').hide();

    $.ajax({
      type: 'POST',
      url: '/cart/clear.js',
      dataType: 'json',
    });

  }
},100);

function HideNav()
{
  if(screen.width < 800)
  {	
    $("#AppFrameNav").hide();
  }
}

