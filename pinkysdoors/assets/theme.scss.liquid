.already-visible {
  opacity: 1;
  visibility: visible;
}
.anim-in--slide-from-right {
  @include transform(translateX(100px));
  opacity: 0;
  &.bring-it {
    visibility: visible;
    animation: slide-in-from-right 0.6s ease forwards;
  }
}
.anim-in--slide-from-left {
  @include transform(translateX(-100px));
  opacity: 0;
  &.bring-it {
  	animation: slide-in-from-left 0.6s ease forwards;
    visibility: visible;
  }
}

.anim-in--slide-up {
  transform: translateY(50px);
  opacity: 0;
  &.bring-it {
  	animation: slide-up 0.6s ease forwards;
    visibility: visible;
    &.i2 {
      animation-delay: .3s !important;
    }
    &.i3 {
      animation-delay: .6s !important;
    }
      
  }
  
}



@keyframes slide-in-from-right {
  to {@include transform(translateX(0)); opacity: 1;  }
}
@keyframes slide-in-from-left {
  to { @include transform(translateX(0)); opacity: 1;  }
}
@keyframes slide-up {
  to { transform: translateY(0); opacity: 1;  }
}


