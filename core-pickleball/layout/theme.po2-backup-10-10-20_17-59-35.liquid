</html>

 <script>
    $('document').ready(function(){
      $('.dbtfy-color_swatches .swatch .swatch-element label[for=swatch-1-yellow]').click(function(){
        $(".single-option-selector__radio option[value='24 Core Pickleballs (Out of Stock)']").css('display', 'none');
        $(".single-option-selector__radio option[value='50 Core Pickleballs (pre-sale)']").css('display', 'none');
        $(".single-option-selector__radio option[value='100 Core Pickleballs (pre-sale)']").css('display', 'none');
        
      });
      $('.dbtfy-color_swatches .swatch .swatch-element label[for=swatch-1-green]').click(function(){
        $(".single-option-selector__radio option[value='24 Core Pickleballs (Out of Stock)']").css('display', 'block');
        $(".single-option-selector__radio option[value='50 Core Pickleballs (pre-sale)']").css('display', 'block');
        $(".single-option-selector__radio option[value='100 Core Pickleballs (pre-sale)']").css('display', 'block');
      });
    });
    
  </script>